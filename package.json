{
	"name": "venga-server",
	"version": "1.0.0",
	"description": "This is a boilerplate project used for starting new projects!",
	"main": "src/server.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "mocha --require test/setup.js",
		"start": "node src/server.js",
		"dev": "nodemon src/server.js",
		"migrate": "postgrator --config postgrator-config.js",
		"migrate:test": "DATABASE_URL=postgresql://andreabender@localhost/venga-test npm run migrate",
		"migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
		"seed": "psql -U andreabender -d venga -a -f seeds/seed.tables.sql",
		"seed:test": "psql -U andreabender -d venga-test -a -f seeds/seed.tables.sql",
		"predeploy": "npm audit && npm run migrate:production",
		"deploy": "git push heroku master"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^6.2.2",
		"nodemon": "^1.19.4",
		"postgrator-cli": "^3.2.0",
		"supertest": "^4.0.2"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"helmet": "^3.21.2",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.16.3",
		"migrate": "^1.6.2",
		"morgan": "^1.9.1",
		"pg": "^7.12.1",
		"xss": "^1.0.3"
	}
}
